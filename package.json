{
  "name": "svelte-pixi",
  "version": "0.1.3",
  "description": "Create PixiJS apps with Svelte",
  "repository": "https://github.com/mattjennings/svelte-pixi",
  "author": "Matt Jennings",
  "license": "MIT",
  "scripts": {
    "build": "npm run package",
    "package": "svelte-package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "validate": "svelte-check --tsconfig ./tsconfig.json --threshold error --ignore \"dist\"",
    "prepare": "husky install",
    "release": "release-it --npm.tag=latest",
    "prerelease": "release-it --preRelease=beta --npm.tag=next",
    "lint-staged": "lint-staged",
    "lint": "eslint .",
    "prettier": "prettier --ignore-path .eslintignore . --check",
    "format": "prettier --ignore-path .eslintignore . --write"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "@sveltejs/adapter-vercel": "^3.1.0",
    "@sveltejs/kit": "^1.27.6",
    "@sveltejs/package": "^2.2.2",
    "@tailwindcss/typography": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "^10.4.4",
    "css-tree": "^2.3.1",
    "dashify": "^2.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-svelte": "^2.35.0",
    "fathom-client": "^3.4.1",
    "husky": "^7.0.0",
    "just-camel-case": "^6.0.1",
    "lint-staged": "^15.1.0",
    "mdsvex": "^0.11.0",
    "mdsvex-sveld": "^1.0.3",
    "mdsvexamples": "^0.4.1",
    "mousetrap": "^1.6.5",
    "pixi.js": "^7.3.2",
    "postcss": "^8.4.12",
    "postcss-load-config": "^3.1.3",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.0",
    "prism-svelte": "^0.5.0",
    "prismjs": "^1.27.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "release-it": "^15.0.0",
    "sveld": "^0.16.0",
    "svelte": "^4.2.5",
    "svelte-check": "^3.6.0",
    "svelte-focus-trap": "^1.2.0",
    "svelte-intersection-observer": "^0.10.0",
    "svelte-preprocess": "^5.1.0",
    "svelte2tsx": "^0.6.25",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.3.1",
    "typescript": "^5.2.0",
    "unist-util-visit": "^4.1.0",
    "vite": "^4.0.0",
    "vite-plugin-sveld": "^1.1.0"
  },
  "peerDependencies": {
    "pixi.js": ">=7.3.2",
    "svelte": ">=3.0.0 || >=4.0.0"
  },
  "svelte": "./dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./experimental": {
      "types": "./dist/experimental/index.d.ts",
      "default": "./dist/experimental/index.js"
    }
  },
  "typesVersions": {
    ">=4.0": {
      "*": [
        "dist/*"
      ]
    }
  }
}
